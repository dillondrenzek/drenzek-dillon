//- List Skills
//- version 0.5.0
//- arguments:
	objects: Array - list of all the skills to display
extends ./skills.jade

block skill_page
	.main-page
		h1 Skills
			//- span= objects.length
		+list(objects)
			each skill in objects
				+item(skill, false)(onclick="location.href='/skills/#{skill.id}'")